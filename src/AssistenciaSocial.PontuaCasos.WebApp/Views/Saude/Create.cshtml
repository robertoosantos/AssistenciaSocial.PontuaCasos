@model IEnumerable<AssistenciaSocial.PontuaCasos.WebApp.Models.Item>

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Situações de Saúde</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" asp-route-individuo_id="@Context.Request.Query["individuo_id"]">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <partial name="~/Views/Shared/_ItemSelect.cshtml" model="@Model" />
            <p class="text-danger fw-bold">@ViewData["Erro"]</p>
            <input type="submit" value="Create" class="btn btn-primary" />
        </form>
    </div>
</div>

<div>
    <a href="javascript:void(0);" onclick="history.go(-1);">Back to List</a>
</div>
